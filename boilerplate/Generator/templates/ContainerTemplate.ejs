import React, {Component} from 'react'
import {connect} from 'react-redux'
import {Container} from 'native-base'
import {Colors} from '<%= levels %>Themes'
import LoginActions from "<%= levels %>Redux/LoginRedux"
import AppHeader from "<%= levels %>Components/Header"
import {Content} from "<%= levels %>Components/Content"

class <%= name %> extends Component {

	render() {
		return (
			<Container>

				<AppHeader title={"<%= title %>"} />

				<Content padder>

				</Content>
			</Container>
		)
	}
}

const mapStateToProps = (state) => {
	return {
		settings: state.settings.data,
		login: state.login.data
	}
}

const mapDispatchToProps = dispatch => {
	return {
		saveLoginData: data => {
			dispatch(LoginActions.loginSuccess(data))
		}
	}
}

export default connect(mapStateToProps, mapDispatchToProps)(<%= name %>)
