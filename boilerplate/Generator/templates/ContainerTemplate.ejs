import React, {Component} from 'react'
import {connect} from 'react-redux'
import {Content} from 'native-base'
import {Colors} from '<%= levels %>Themes'
import LoginActions from "<%= levels %>Redux/LoginRedux"
import {AppHeader} from "<%= levels %>Components/UiComponents"
import {BackgroundContainer} from "<%= levels %>Components/Container"

class <%= name %> extends Component {

	render() {
		const {navigation} = this.props
		const {navigate} = navigation
		return (
			<BackgroundContainer>

				<AppHeader title={"<%= title %>"} />

				<Content style={{ backgroundColor: Colors.transparent, marginTop: 40 }} padder>

				</Content>
			</BackgroundContainer>
		)
	}
}

const mapStateToProps = (state) => {
	return {
		settings: state.settings.data,
		login: state.login.data
	}
}

const mapDispatchToProps = dispatch => {
	return {
		saveLoginData: data => {
			dispatch(LoginActions.loginSuccess(data))
		}
	}
}

export default connect(mapStateToProps, mapDispatchToProps)(<%= name %>)
